{
  "name": "food-nutrition-api",
  "version": "1.0.0",
  "description": "API REST para gestão de alimentos e composição nutricional (TACO/TBCA)",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "deploy:prepare": "npm ci && npx prisma generate && npm run build",
    "deploy:migrate": "npx prisma migrate deploy",
    "health:check": "curl -s http://localhost:3000/health | node -e \"const d=require('fs').readFileSync(0,'utf8');console.log(JSON.parse(d).status)\"",
    "seed:taco": "ts-node src/scripts/seed-taco.ts",
    "seed:tbca": "ts-node src/scripts/seed-tbca.ts",
    "seed:complete": "ts-node src/scripts/generate-complete-data.ts",
    "seed:batch": "ts-node src/scripts/seed-batch.ts",
    "seed:all": "ts-node src/scripts/seed-all.ts",
    "seed:cleanup": "ts-node src/scripts/cleanup.ts",
    "seed:reset": "ts-node src/scripts/reset-db.ts",
    "seed:real": "ts-node src/scripts/seed-real.ts",
    "scraper:taco": "ts-node src/scripts/scraper-taco.ts",
    "scraper:tbca": "ts-node src/scripts/scraper-tbca.ts",
    "import:real": "ts-node src/scripts/import-real-data.ts",
    "import:taco-csv": "ts-node src/scripts/import-taco-csv.ts",
    "import:continue": "ts-node src/scripts/import-off-continue.ts",
    "import:tbca": "ts-node src/scripts/import-tbca-full.ts",
    "import:tbca-smart": "ts-node src/scripts/import-tbca-smart.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.8.1",
    "csv-parse": "^5.5.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "puppeteer": "^24.34.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
